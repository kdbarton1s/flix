<%= form_for([@movie, @review]) do |f| %>
  <%= render "movies/errors", object: @review %>
  <div class="">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="">
    <%= f.label :location %>
    <%= f.text_field :location %>
  </div>
  <div class="">
    <%= f.label :stars %>
    <%= f.select :stars, Review::STARS, prompt: "Pick One" %>
  </div>
  <div class="">
    <%= f.label :comment %>
    <%= f.text_area :comment %>
  </div>
  <%= f.submit "Post Review" %> | <%= link_to("Cancel", @movie) %>
<% end %>
